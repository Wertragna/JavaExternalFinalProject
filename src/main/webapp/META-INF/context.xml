<Context antiJARLocking="true">
    <Resource
            name="jdbc/enrollment_db"
            author="Container"
            type="javax.sql.DataSource"
            maxActive="32"
            maxIdle="8"
            maxWait="10000"
            username="root"
            password="root"
            url="jdbc:mysql://localhost:3306/enrollment_db?serverTimezone=UTC"
            driverClassName="com.mysql.cj.jdbc.Driver"
    />
</Context>